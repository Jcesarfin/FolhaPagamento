<Window x:Name="telaManutFolha" x:Class="FolhaPagamento.ManutencaoFolha"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FolhaPagamento"
        mc:Ignorable="d"
        Title="MANUTENÇÃO FOLHA PAGAMENTO" Background="#FFF17272" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblManutFolhaCpf" Content="INFORME O CPF:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtManutFolhaCpf" HorizontalAlignment="Left" Height="23" Margin="114,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <Button x:Name="btnConsFolhaCpf" Content="PESQUISAR" HorizontalAlignment="Left" Margin="374,10,0,0" VerticalAlignment="Top" Width="75" Click="btnConsFolhaCpf_Click"/>
        <Label x:Name="lblManutFolhaNome" Content="NOME:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblManutMesFolha" Content="MÊS:" HorizontalAlignment="Left" Margin="10,315,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtManutMesFolha" HorizontalAlignment="Left" Height="23" Margin="210,320,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67"/>
        <TextBox x:Name="txtManutNomeFolha" HorizontalAlignment="Left" Height="23" Margin="114,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="335"/>
        <Label x:Name="lblManutSalBrFolha" Content="VALOR BRUTO:" HorizontalAlignment="Left" Margin="10,196,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblManutInssFolha" Content="VALOR INSS:" HorizontalAlignment="Left" Margin="10,227,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtManutSalBrFolha" HorizontalAlignment="Left" Height="23" Margin="157,199,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtManutInssFolha" HorizontalAlignment="Left" Height="23" Margin="157,230,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblManutIrFolha" Content="VALOR IR:" HorizontalAlignment="Left" Margin="10,258,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtManutIrFolha" HorizontalAlignment="Left" Height="23" Margin="157,261,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblManutVlrLiqFolha" Content="VALOR LIQUIDO:" HorizontalAlignment="Left" Margin="10,289,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtManutVlrLiqFolha" HorizontalAlignment="Left" Height="23" Margin="157,292,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnVoltarMenuInicial" Content="MENU INICIAL" HorizontalAlignment="Left" Margin="10,377,0,0" VerticalAlignment="Top" Width="105" Click="btnVoltarMenuPrinc_Click"/>
        <Button x:Name="btnVoltarMenuManut" Content="MENU MANUTENÇÃO" HorizontalAlignment="Left" Margin="157,377,0,0" VerticalAlignment="Top" Width="126" Click="btnVoltarMenuManut_Click"/>
        <Label x:Name="lblManutQtdeHrNormal" Content="QUANTIDADE HORA NORMAL:" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblManutQtdeHrExtra" Content="QUANTIDADE HORA EXTRA:" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblManutVlrHrNormal" Content="VALOR HORA NORMAL:" HorizontalAlignment="Left" Margin="10,134,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblManutVlrHrExtra" Content="VALOR HORA EXTRA:" HorizontalAlignment="Left" Margin="10,165,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblManutAnoFolha" Content="ANO:" HorizontalAlignment="Left" Margin="10,341,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtManutAnoFolha" HorizontalAlignment="Left" Height="23" Margin="210,348,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67"/>
        <TextBox x:Name="txtManutQtHrNormal" HorizontalAlignment="Left" Height="23" Margin="200,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="77"/>
        <TextBox x:Name="txtManutQtHrExtra" HorizontalAlignment="Left" Height="23" Margin="200,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="77"/>
        <TextBox x:Name="txtManutVlrHrNormal" HorizontalAlignment="Left" Height="23" Margin="157,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtManutVlrHrExtra" HorizontalAlignment="Left" Height="23" Margin="157,168,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnFolhaEditar" Content="EDITAR" HorizontalAlignment="Left" Margin="374,320,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnFolhaExcluir" Content="EXCLUIR" HorizontalAlignment="Left" Margin="374,351,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
