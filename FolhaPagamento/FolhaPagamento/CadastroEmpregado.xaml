<Window x:Name="telaCadastroEmpregado" x:Class="FolhaPagamento.CadastroEmpregado"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FolhaPagamento"
        mc:Ignorable="d"
        Title="Cadastro de Empregados" Height="300" Width="300">
    <Grid>
        <Label x:Name="lblTitulo" Content="INFORME OS DADOS:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblNomeEmpregado" Content="NOME:" HorizontalAlignment="Left" Margin="12,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblIdentidade" Content="NR. IDENTIDADE:" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblCPF" Content="NR. CPF:" HorizontalAlignment="Left" Margin="10,93,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDataNascimento" Content="DATA NASCIMENTO:" HorizontalAlignment="Left" Margin="10,124,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDataAdmissao" Content="DATA ADMISSÃO" HorizontalAlignment="Left" Margin="10,149,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDataDemissao" Content="DATA DEMISSÃO" HorizontalAlignment="Left" Margin="12,181,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNome" HorizontalAlignment="Left" Height="23" Margin="134,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtNrIdentidade" HorizontalAlignment="Left" Height="23" Margin="134,67,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtNrCpf" HorizontalAlignment="Left" Height="23" Margin="134,95,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtDtNascimento" HorizontalAlignment="Left" Height="23" Margin="134,124,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtDtAdmissao" HorizontalAlignment="Left" Height="23" Margin="134,152,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtDtDemissao" HorizontalAlignment="Left" Height="23" Margin="134,180,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnSalvarCadEmpregado" Content="SALVAR" HorizontalAlignment="Left" Margin="179,208,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnMigrarCadEndereco" Content="CADASTRAR ENDEREÇO" HorizontalAlignment="Left" Margin="12,239,0,0" VerticalAlignment="Top" Width="143"/>
        <Button x:Name="btnVoltarMenuCadastro" Content="VOLTAR MENU" HorizontalAlignment="Left" Margin="169,239,0,0" VerticalAlignment="Top" Width="85"/>

    </Grid>
</Window>
