<Window x:Name="TelaManutGeral" x:Class="FolhaPagamento.ManutencaoGeral"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FolhaPagamento"
        mc:Ignorable="d"
        Title="MANUTENÇÃO GERAL" Background="#FFF17272">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="35*"/>
            <ColumnDefinition Width="38*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnManutCadastro" Content="MANUTENÇÃO CADASTRO" HorizontalAlignment="Left" Margin="10,51,0,0" VerticalAlignment="Top" Width="242" Click="btnManutCadastro_Click" FontWeight="Bold" FontSize="16"/>
        <Button x:Name="btnManutFolha" Content="MANUTENÇÃO FOLHA" HorizontalAlignment="Left" Margin="10,96,0,0" VerticalAlignment="Top" Width="242" Click="btnManutFolha_Click" FontWeight="Bold" FontSize="16"/>
        <Button x:Name="btnVoltarMenu" Content="MENU INICIAL" HorizontalAlignment="Left" Margin="10,183,0,0" VerticalAlignment="Top" Width="114" Click="btnVoltarMenu_Click" FontWeight="Bold"/>

    </Grid>
</Window>
